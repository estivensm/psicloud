<div class="col-md-12 " style=" padding-left: 5px;">
    <div id='cssmenu' style="margin-top: 10px" >
      <ul>
          <li><%= link_to "Inicio", root_path%></li> 
          <li class="active"><%= link_to "Mi Cuenta", account_path%></li>
      </ul>
    </div>
</div>
<br><br><br>

<body class="w3-light-grey">

<!-- Page Container -->
<div class="w3-content w3-margin-top" style="max-width:1400px;">

  <!-- The Grid -->
  <div class="w3-row-padding">
  
    <!-- Left Column -->
    <div class="w3-third" style="width: 320px;">
    
      <div class="w3-white w3-text-grey w3-card-4">
        <div class="w3-display-container" >
          <% if @account.logo? %>
        
            <%= image_tag(@account.logo_url(:medium))%>
          <%else%>
            <%= image_tag "https://mybc1.s3.amazonaws.com/uploads/rseguimiento/evidencia/247/no-imagen.jpg", style: "width:294px"  %>

        <%end%>
        </div>
        <br><br>
        <div class="w3-container">
          <p><i class="fa fa-user fa-fw w3-margin-right w3-large w3-text-teal" style="color: #257293 !important;"></i><%= @account.name%></p>
          <p><i class="fa fa-address-card fa-fw w3-margin-right w3-large w3-text-teal" style="color: #257293 !important;"></i><%= @account.nit%></p>
          <p><i class="fa fa-envelope fa-fw w3-margin-right w3-large w3-text-teal" style="color: #257293 !important;"></i><%= @account.email %></p>
          <p><i class="fa fa-phone fa-fw w3-margin-right w3-large w3-text-teal" style="color: #257293 !important;"></i> <%= @account.phone %></p>
          
          <hr>
             <p style="padding:10px;"><%= link_to  edit_account_path(@account),:class => "btn btn-primary pull-right" do %>
                           <i class="glyphicon glyphicon-pencil"></i>
                           <span >Editar Cuenta</span>
              <% end %>
                  
               <%= link_to  root_path, :class => "btn btn-default pull-right" do %>
                           <i class="fas fa-back"></i>
                           <span >Volver</span>
              <% end %></p>
          <br>

        </div>
      </div><br>

    <!-- End Left Column -->
    </div>

    <!-- Right Column -->
    <div class="w3-twothird">
    
      <div class="w3-container w3-card w3-white w3-margin-bottom">
        <h2 class="w3-text-grey w3-padding-16"><i class="fa fa-suitcase fa-fw w3-margin-right w3-xxlarge w3-text-teal" style="color: #257293 !important;"></i> <%= @account.name%></h2>
        <div class="w3-container">

          <div class="row">
                  <div class="col-md-6">
              <div class="panel panel-default">
                 <div class="panel-heading">
                     <i class="fa fa-database" aria-hidden="true" style=""></i> Plan
                 </div>
                 <div class="panel-body">
                      <p style="text-align: center"><span class="" style="    color: #46b8da;font-size:25px;">Psicologo</span></p>
                 </div>
              </div>
                
              </div>
              <div class="col-md-6">
              <div class="panel panel-default">
                 <div class="panel-heading">
                     <i class="fa fa-database" aria-hidden="true" style=""></i> Pacientes Atendidos
                 </div>
                 <div class="panel-body">
                      <p style="text-align: center"><span class="badge" style="font-size:32px;"><%=Patient.where(admin_user: current_user.admin_user).count%>11</span></p>
                 </div>
              </div>
                
              </div>  

              <div class="col-md-6">
              <div class="panel panel-default">
                 <div class="panel-heading">
                     <i class="fa fa-user" aria-hidden="true" style=""></i> Usuarios 1/1
                 </div>
                 <div class="panel-body">

                      <div class="progress">
                          <div class="progress-bar progress-bar-info" role="progressbar" aria-valuenow="100"
                                  aria-valuemin="0" aria-valuemax="100" style="width:100%">
                                  100% Usado
                          </div>
                      </div>
                 </div>
              </div>
                
              </div>
              <div class="col-md-6">
              <div class="panel panel-default">
                 <div class="panel-heading">
                     <i class="fa fa-database" aria-hidden="true" style=""></i> Almacenamiento 0 / 10 GB
                 </div>
                 <div class="panel-body">
                      <div class="progress">
                          <div class="progress-bar progress-bar-info" role="progressbar" aria-valuenow="40"
                                  aria-valuemin="0" aria-valuemax="100" style="width:10%">
                                  1% Usado
                          </div>
                      </div>
                 </div>
              </div>
                
              </div>

            
          </div>
          

          <hr>
        </div>
 
      </div>


    <!-- End Right Column -->
    </div>
    
  <!-- End Grid -->
  </div>
  
  <!-- End Page Container -->
</div>



<br><br><br>

<div class="container">
  

<div class="row" >
  <br>
</div>  
  <div class="row">
    <div class="col-md-12">
      <div class="panel panel-default">
        <div class="panel-body">
          <div class="row">
            <div class="col-md-12 lead">Informacion de la Cuenta
          
            <hr></div>
          </div>
          <div class="row" style="color: #5bc0de !important;">
          <div class="col-md-4 text-center">
      <% if @account.logo? %>
        
       <%= image_tag(@account.logo_url(:medium))%>
        <%else%>
      <%= image_tag "http://michigrafias.com/wp-content/uploads/2018/04/no-imagen.jpg", style: "width:200px"  %>

        <%end%>
            </div>
            <div class="col-md-8">
              <div class="row">
                <div class="col-md-12">
                  <h1 class="only-bottom-margin"><%= @account.name%></h1>
                </div>
              </div>
              <div class="row">
                <div class="col-md-6">
                  <p class="info"><strong>Email: </strong> <%= @account.email %></p>
                  <p class="info"><strong>Telefono:</strong> <%= @account.phone %></p>
                  <p class="info"><strong>Direcci√≥n: </strong> <%= @account.address %></p>
                  <p class="info"><strong>Nit/Cedula:</strong> <strong></p>               
                </div>
                <div class="col-md-6 text-center">
                  <div class="activity-mini">
                    Pacientes Atendidos <br> <span><i class="glyphicon glyphicon-user text-muted"></i> <%= @patiens.count %></span>
                  </div>
               
                </div>
              </div>
            </div>
          </div>
          <hr>
          <div class="row">
            <div class="col-md-6">
              
              <div class="alert alert-info text-left" style="font-size:25px;">
                <strong>PLAN:</strong> PSICOLOGO
              </div>

              
           
            </div>
          </div>
           <div class="row ">
            <div class="col-md-6" style="padding: 10px 110px;">
             
              <div class="alert alert-success text-left text-center" style="font-size: 30px;">
               <i class="fa fa-user" aria-hidden="true" style="font-size: 60px;"></i><br>
                <strong>USUARIOS:</strong><br> 1 / 1 
              </div>

              
           
            </div>
             <div class="col-md-6" style="padding: 10px 110px;">
              
              <div class="alert alert-success text-left text-center" style="font-size: 30px;">
               <i class="fa fa-database" aria-hidden="true" style="font-size: 60px;"></i><br>
                <strong>ALMACENAMIENTO:</strong><br> 1GB / 20GB
              </div>

              
           
            </div>
          </div>
        </div>
      </div>
    </div>
  </div>
</div>
