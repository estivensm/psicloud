<p id="notice"><%= notice %></p>

<h1>Clinic Histories</h1>

<table>
  <thead>
    <tr>
      <th>Clinic history</th>
      <th>Count</th>
      <th>Consultation reason</th>
      <th>Actual state</th>
      <th>Diagnostic hypothesis</th>
      <th>Therapeutic goal</th>
      <th>Quantity appointment</th>
      <th>Frequency appointment</th>
      <th>User</th>
      <th>Admin user</th>
      <th>Patient</th>
      <th>Type of treatment</th>
      <th>Description appointment</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @clinic_histories.each do |clinic_history| %>
      <tr>
        <td><%= clinic_history.clinic_history_id %></td>
        <td><%= clinic_history.count %></td>
        <td><%= clinic_history.consultation_reason %></td>
        <td><%= clinic_history.actual_state %></td>
        <td><%= clinic_history.diagnostic_hypothesis %></td>
        <td><%= clinic_history.therapeutic_goal %></td>
        <td><%= clinic_history.quantity_appointment %></td>
        <td><%= clinic_history.frequency_appointment %></td>
        <td><%= clinic_history.user_id %></td>
        <td><%= clinic_history.admin_user %></td>
        <td><%= clinic_history.patient_id %></td>
        <td><%= clinic_history.type_of_treatment %></td>
        <td><%= clinic_history.description_appointment %></td>

      </tr>
    <% end %>
  </tbody>
</table>

<br>
