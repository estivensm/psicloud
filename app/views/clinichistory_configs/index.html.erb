<p id="notice"><%= notice %></p>

<h1>Clinichistory Configs</h1>

<table>
  <thead>
    <tr>
      <th>Header image</th>
      <th>Footer image</th>
      <th>Header text</th>
      <th>Footer text</th>
      <th>State</th>
      <th>Admin user</th>
      <th>User</th>
      <th>User</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @clinichistory_configs.each do |clinichistory_config| %>
      <tr>
        <td><%= image_tag clinichistory_config.header_image_url, style: "width:100px"  if clinichistory_config.header_image_url %></td>
        <td><%= image_tag clinichistory_config.footer_image_url, style: "width:100px" if clinichistory_config.footer_image_url  %></td>
        <td><%== clinichistory_config.header_text %></td>
        <td><%= clinichistory_config.footer_text %></td>
        <td><%= clinichistory_config.state %></td>
        <td><%= clinichistory_config.admin_user %></td>
        <td><%= clinichistory_config.user.email %></td>
        <td><%= clinichistory_config.user_id %></td>
        <td><%= link_to 'Show', clinichistory_config %></td>
        <td><%= link_to 'Edit', edit_clinichistory_config_path(clinichistory_config) %></td>
        <td><%= link_to 'Destroy', clinichistory_config, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Clinichistory Config', new_clinichistory_config_path %>
