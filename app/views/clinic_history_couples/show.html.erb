<div class="col-md-12">
      <h1 class="titulo" align="center">INFORMACION GENERAL</h1>
    </div>

      <div class="row">
<div class="col-md-6 text-center paciente2" style="border-right: 1px solid #e7e7e7">
   <div class="col-md-3 imagen1">
      <%=  @clinic_history_couple.patient.avatar? ? image_tag(@clinic_history_couple.patient.avatar_url(:logo)) : image_tag("https://dnuba.s3.amazonaws.com/uploads/bank_image/achivo/44/user.jpeg")%>
   </div>
   <div class="col-md-7 info">
      <h3 align="left"><%= @clinic_history_couple.patient.first_name %> <%= @clinic_history_couple.patient.second_name%></h3>
      <p><strong>Documento:</strong> <%= @clinic_history_couple.patient.document %></p>
      <p style="display: contents;"><strong>Edad: </strong><%= @clinic_history_couple.patient.age %></p>
   </div>
  
</div>
<div class="col-md-6 text-center paciente2 elements">
   <div class="col-md-3 imagen1">
      <%=  @clinic_history_couple.couple_patient.avatar? ? image_tag(@clinic_history_couple.couple_patient.avatar_url(:logo)) : image_tag("https://dnuba.s3.amazonaws.com/uploads/bank_image/achivo/44/user.jpeg")%>
   </div>
   <div class="col-md-7 info">
      <h3 align="left"><%= @clinic_history_couple.couple_patient.first_name %> <%= @clinic_history_couple.couple_patient.second_name%></h3>
      <p><strong> Documento: </strong><%= @clinic_history_couple.couple_patient.document %></p>
      <p style="display: contents;"><strong>Edad: </strong> <%= @clinic_history_couple.couple_patient.age %></p>
   </div>
 
</div>

      </div>



      <div class="row">

        <div class="col-sm-6">
            <h4 class="title">Numero de uniones anteriores</h4>
            <p ><%= @clinic_history_couple.previous_unions_patient %> Uniones</p>

            <h4 class="title">Numero de hijos de anteriores relaciones</h4>
            <p ><%= @clinic_history_couple.previous_children_patient %> Hijos</p>
             <h4 class="title">Conducta Problematica del Paciente</h4>
            <p ><%= @clinic_history_couple.problematic_conduct_patient %></p>
        </div>
      

        <div class="col-sm-6">
            <h4 class="title">Previas Uniones del Paciente</h4>
            <p><%= @clinic_history_couple.previous_unions_couple %> Uniones</p>

            <h4 class="title">Numero de hijos de anteriores relaciones</h4>
            <p><%= @clinic_history_couple.previous_children_couple %> Hijos</p>

                <h4 class="title">Conducta problem√°tica en la pareja</h4>
            <p ><%= @clinic_history_couple.problematic_conduct_couple %></p>
        </div>

      </div>
<hr>
      <div class="row">

        <div class="col-sm-12">
          <h3 class="title">Motivo de Consulta</h3>
            <p><%= @clinic_history_couple.consultation_reason %></p>
        </div>

        <div class="col-sm-12">
            <h3 class="title">Obsevaciones Generales</h3>
          <p><%= @clinic_history_couple.general_observations %></p>
        </div>

        <div class="col-sm-12">
          <h3 class="title">Situacion Actual</h3>
          <p><%= @clinic_history_couple.actual_situations %></p>
        </div>

      </div>
   <div class="row">
      
       <div class="col-sm-12">
          <h3 class="title">Diagnostico</h3>
          <h5 class="">Descripcion</h5>
          <p><%= @clinic_history_couple_outcome.diagnostic_description %></p>
        </div>
        <div class="col-sm-12">
          <hr>
            <h5 class="">DESM</h5>
            <p> 
              <% @clinic_history_couple_outcome.diagnostic.each do |diagnostico| %>
             
               <div class="diagnostico" style="margin-bottom: 5px;display: inline-block"><%=diagnostico.name%></div>
              <% end -%>
            </p>
        </div>

        <div class="col-sm-12">
            <hr>
            <h5 class="">CIE10</h5>
            <p>
              <% @clinic_history_couple_outcome.first_diagnostico.each do |diagnostico1| %>
           
               <div class="diagnostico" style="margin-bottom: 5px;display: inline-block"><%=diagnostico1.codigo%> - <%=diagnostico1.name%></div>
              <% end -%>
            </p>
        </div>

         <div class="col-sm-12">
          <h3 class="title">Objetivo terapeutico</h3>
          <p><%= @clinic_history_couple_outcome.therapeutic_goal %></p>
        </div>
        

           <div class="col-sm-12">
          <h3 class="title">Tipo de Tratamiento</h3>
          <p><%= @clinic_history_couple_outcome.type_of_treatment %></p>
        </div>  

    </div>
